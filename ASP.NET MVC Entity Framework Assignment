public ActionResult Create(Insuree insuree)
{
    // Start with a base quote
    decimal quote = 50;

    // Age-based calculations
    if (insuree.Age <= 18)
    {
        quote += 100;
    }
    else if (insuree.Age >= 19 && insuree.Age <= 25)
    {
        quote += 50;
    }
    else if (insuree.Age >= 26)
    {
        quote += 25;
    }

    // Car year calculations
    if (insuree.CarYear < 2000)
    {
        quote += 25;
    }
    else if (insuree.CarYear > 2015)
    {
        quote += 25;
    }

    // Make and model calculations
    if (insuree.CarMake.Equals("Porsche", StringComparison.OrdinalIgnoreCase))
    {
        quote += 25; // Base for Porsche
        if (insuree.CarModel.Equals("911 Carrera", StringComparison.OrdinalIgnoreCase))
        {
            quote += 25; // Additional for 911 Carrera
        }
    }

    // Speeding tickets calculations
    quote += insuree.SpeedingTickets * 10;

    // DUI calculations
    if (insuree.DUI)
    {
        quote *= 1.25m; // Add 25%
    }

    // Full coverage calculations
    if (insuree.FullCoverage)
    {
        quote *= 1.50m; // Add 50%
    }

    // Save quote
    insuree.Quote = quote;

    // Save to database (your existing logic here)

    return RedirectToAction("Index"); // or your appropriate view
}
@model YourNamespace.Models.Insuree

@using (Html.BeginForm())
{
    @Html.LabelFor(m => m.FirstName)
    @Html.TextBoxFor(m => m.FirstName)

    @Html.LabelFor(m => m.LastName)
    @Html.TextBoxFor(m => m.LastName)

    @Html.LabelFor(m => m.Age)
    @Html.TextBoxFor(m => m.Age)

    @Html.LabelFor(m => m.CarYear)
    @Html.TextBoxFor(m => m.CarYear)

    @Html.LabelFor(m => m.CarMake)
    @Html.TextBoxFor(m => m.CarMake)

    @Html.LabelFor(m => m.CarModel)
    @Html.TextBoxFor(m => m.CarModel)

    @Html.LabelFor(m => m.SpeedingTickets)
    @Html.TextBoxFor(m => m.SpeedingTickets)

    @Html.LabelFor(m => m.DUI)
    @Html.CheckBoxFor(m => m.DUI)

    @Html.LabelFor(m => m.FullCoverage)
    @Html.CheckBoxFor(m => m.FullCoverage)

    <input type="submit" value="Create" />
}
@model IEnumerable<YourNamespace.Models.Insuree>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var insuree in Model)
        {
            <tr>
                <td>@insuree.FirstName</td>
                <td>@insuree.LastName</td>
                <td>@insuree.Email</td>
                <td>@insuree.Quote</td>
            </tr>
        }
    </tbody>
</table>
public ActionResult Admin()
{
    var allInsurees = db.Insurees.ToList(); // Adjust to your data context
    return View(allInsurees);
}
